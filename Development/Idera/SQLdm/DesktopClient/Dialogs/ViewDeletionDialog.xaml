<Window x:Class="Idera.SQLdm.DesktopClient.Dialogs.ViewDeletionDialog"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Idera.SQLdm.DesktopClient.Dialogs"
             mc:Ignorable="d" 
            Width="300"
            Height="200"
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel>
        <TextBlock HorizontalAlignment="Center" Foreground="Black" FontSize="14"  FontFamily="{StaticResource SourceSansPro}" Text="Which view would you like to delete?"></TextBlock>
        <ComboBox Name="viewNamesCombobox" Width="200" Margin="0,10,0,0"></ComboBox>
        <Button x:Name="okButton" Margin="10" Content="Delete" Click="OkButton_Click" Width="100" Style="{DynamicResource TabActionButtonStyle}"></Button>
    </StackPanel>
</Window>
