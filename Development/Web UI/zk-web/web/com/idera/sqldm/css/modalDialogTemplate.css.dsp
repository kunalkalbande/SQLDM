.modal-dialog-window {
	border: 0;
}

.modal-dialog-title {
	font-weight: bold;
	font-size: 16pt;
	line-height: 18pt;
	color: #363636;
}

.modal-dialog-subtitle {
	font-weight: bold;
	font-size: 12pt;
	line-height: 12.5pt;
	color: #363636;
}

.modal-dialog-close-link {
	font-size: 10pt;
	text-decoration: none;
	color: #545455;
}

