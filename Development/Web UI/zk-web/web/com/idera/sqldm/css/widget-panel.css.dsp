
.z-panel.widget-panel { 
	border: 1px solid #aaaaaa;
	-webkit-box-shadow: 2px 2px 3px #DDD;
	box-shadow: 2px 2px 3px #DDD;
	overflow: auto;
}

.z-panel.widget-panel .z-caption-content,
.z-panel.widget-panel > .z-panel-tl { 
	display: none;
}

.z-panel.widget-panel > .z-panel-hl,
.z-panel.widget-panel > .z-panel-hl > .z-panel-hr,
.z-panel.widget-panel > .z-panel-head,
.z-panel.widget-panel > .z-panel-head > .z-panel-header {
	padding: 0;
}

.z-panel.widget-panel > .z-panel-hl,
.z-panel.widget-panel > .z-panel-hl > .z-panel-hr,
.z-panel.widget-panel > .z-panel-hl > .z-panel-hr > .z-panel-hm,
.z-panel.widget-panel > .z-panel-head,
.z-panel.widget-panel > .z-panel-head > .z-panel-header {
	background-image: none;
	background-repeat: no-repeat !important;
	background-position: center right !important;
	background-color: #ebebeb;
	border-width: 0 0 1px 0;
	border-color: #cccccc;
} 
.z-panel-hr {
    background: transparent no-repeat right 0;
    background-image: url(/zkau/web/d9aeb4e8/zul/img/wnd/panel-hr.png);
    padding-right: 7px;
    zoom: 1;
} 

.z-panel.widget-panel .z-caption .z-label,
.z-panel.widget-panel > .z-panel-hl > .z-panel-hr > .z-panel-hm > .z-panel-header > .z-caption > tbody > tr > .z-caption-l { 
	    font-size: 10pt;
    font-weight: bold;
    color: #363636;
    font-family: "Source Sans Pro", sans-serif;
}

.z-panel.widget-panel > .z-panel-hl > .z-panel-hr > .z-panel-hm > .z-panel-header { 
	padding: 3px;
}

.z-panel.widget-panel > .z-panel-body > .z-panelchildren { 
	padding: 4px 0 0 0;
	overflow: hidden;
}

.z-panel.widget-panel > .z-panel-hl > .z-panel-hr > .z-panel-hm > .z-panel-header > .z-caption > tbody > tr > td.z-caption-r > .z-hlayout { 
	padding: 2px 4px 0 0;
}

.z-panel.widget-panel .z-row-inner { 
	padding: 2px 4px;
}

.z-panelchildren {
overflow: auto;
}
.z-panel-exp{
background-image: url('/images/expand.png') !important;
background-repeat: no-repeat;	
	width: 24px !important;
    height: 24px !important;
    background-position: center right !important;
    margin-right: 0px !important;
}

.z-panel-colpsd .z-panel-exp{
background-image: url('/images/collapse.png') !important;
background-repeat: no-repeat;
	background-position: center right !important;
	width: 24px !important;
    height: 24px !important;
      margin-right: 0px !important;
}

.applyBtn .z-button>table>tbody>tr>td.z-button-cm {
 text-transform: none;
overflow: hidden;
padding: 2px !important;
border:1px solid #d9d9d9 !important;
    border-radius: 2px !important;
    background-color: #f9f9f9 !important;
    border: 0.5px solid #e6e6e6 !important;
    background-image: url(/zkau/web/d9aeb4e8/zul/img/button/btn-ctr.gif) !important;
}
