.box {
    border: 1px solid #a8c8d6;
    background: #f2f9fc;
    background: #f2f9fc url(data:image/svg+xml;base64,);
    background: -moz-linear-gradient(top, #f2f9fc 0%, #d0e8f3 100%);
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#f2f9fc), color-stop(100%,#d0e8f3));
    background: -webkit-linear-gradient(top, #f2f9fc 0%,#d0e8f3 100%);
    background: -o-linear-gradient(top, #f2f9fc 0%,#d0e8f3 100%);
    background: -ms-linear-gradient(top, #f2f9fc 0%,#d0e8f3 100%);
    background: linear-gradient(to bottom, #f2f9fc 0%,#d0e8f3 100%);
    -moz-box-shadow: inset 0px 0px 0px 1px #edf5f8;
    -webkit-box-shadow: inset 0px 0px 0px 1px #edf5f8;
    box-shadow: inset 0px 0px 0px 1px #edf5f8;
}

