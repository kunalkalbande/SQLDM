.nav-bar {
    background-color: transparent;
    /* background-image: url('/images/dark-black-gradient.png'); */
    background-repeat: repeat-x;
    background-color: #70C2D5;
}

div#gE8Sd2 {}


.navigation-bar-height{
    height: 45px;
}

.nav-bar-element {
    white-space: nowrap;
    text-transform: uppercase;
    display: inline-block;
    text-decoration: none;
    cursor: pointer;
    color: #3B3B3B;
    box-sizing: border-box;
}

.nav-bar-element-other-page:hover {
    background-color: #3B3B3B;
}

.nav-bar-element-current-page {
    color: #3B3B3B;
    background-repeat: no-repeat;
    background-position: center top;
    border-bottom: 5px solid #006089;
    /* color: #FE4210; */
    /* background-image: url('/images/nav-bar-down-arrow.png'); */
    background-repeat: no-repeat;
    background-position: center top;
}

.nav-bar-label {
    font-size: 1rem;
    font-weight: bold;
}

.nav-bar-products-label {
    color: #000;
    font-size: 1rem;
    font-weight: bold;
}

.products-nav-bar.nav-bar-height {
    height: 45px;
}

.products-nav-bar .nav-bar-label {
    color: #fff;
    font-size: 1rem;
}

.nav-bar-drop-down { 
   right: 16px;
   position: fixed;
   z-index: 9999;
   background-color: #70C2D5;
   height: 35px;
   margin: 40px 16px 0px 30px;
   transform: translateY(-39px);
}

.products-nav-bar .nav-bar-element-current-page > .nav-bar-label {
    color: #FE4210;
}

.products-nav-bar .nav-bar-element-other-page:hover > .nav-bar-label {
    color: #FE4210;
}