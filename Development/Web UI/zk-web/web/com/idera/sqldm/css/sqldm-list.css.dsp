.clickable-list:active {
	background-image:url('../images/lviewHover.png');
}

.clickable {
  background-repeat:no-repeat;
  display:block;
  height:20px;
  overflow:hidden;
  text-indent:100%;
  white-space:nowrap;
  width:20px;
  padding-right: 5px;
  background-position:center; 
}

.clickable:hover {
	border: 1px solid #CCC;
	background-color: #E1EDF4;
}


.styled-group tr.z-group td.z-group-inner {
	text-align : left;
}

.z-paging {
	background-color: #fff;
	border-width: 0;
}

div.selectable-styled-list .z-grid-body > table > tbody.z-rows > tr.z-row.selected-row > .z-row-inner {
	border-color: #e5f4fb;
	background: -webkit-linear-gradient(top, #e5f4fb 0%, #d3edfa 100%);
	background: linear-gradient(to bottom, #e5f4fb 0%, #d3edfa 100%);
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#e5f4fb', endColorstr='#d3edfa');
    -ms-filter: "progid:DXImageTransform.Microsoft.gradient(startColorstr='#e5f4fb', endColorstr='#d3edfa')";
	background-clip: padding-box;
}

.cell-label-hover {
	display: inline-block;
	text-overflow: ellipsis;
	overflow: hidden;
	white-space: nowrap;
}
