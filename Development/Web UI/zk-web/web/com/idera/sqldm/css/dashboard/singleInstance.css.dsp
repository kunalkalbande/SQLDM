 .z-tab {
    font-size: 16px;
    /* omitted */
 
    &-icon {
        position: static; /* position decided by .z-tab-button, change it from absolute to static */
        /* omitted */
    }
 
    &-text {
        color: #0054a6;
        padding: 7px 12px; /* expand height to 48px by padding */
        line-height: 32px;
        /* omitted */
    }
 
    &-selected {
        /* omitted */
 
        .z-tab-text {
            font-weight: bold;
            color: black;
        }
    }
    &-disabled {
        background: #ACACAC;
        .opacity(1);
        /* omitted */
 
        &:hover {
            .verGradient(#0054a6, #FFFFFF);
        }
        .z-tab-button:hover {
            .opacity(1);
        }
        .z-tab-text {
            color: #0054a6;
            .opacity(1);
            /* omitted */
        }
    }
}
 

