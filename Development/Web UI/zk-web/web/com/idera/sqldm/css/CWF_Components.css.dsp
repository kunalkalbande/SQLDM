 }
 
 .idera-button-primary-dashboard-Details {
    color: #473E2E;
    text-transform: lowercase;
    font-size: 1rem;
    font-family: "Source Sans Pro", sans-serif;
    background-color: #C4BAA3;
    border: none;
    height: 2rem;
    font-weight: 600;
    padding-left: 1rem;
    padding-right: 1rem;
    vertical-align: middle;
    transition: box-shadow .2s ease;
